#include <iostream>

using namespace std;

int main() {
    int num;

    cout << "Ingrese el nÃºmero para la tabla de multiplicar: ";
    cin >> num;

    cout << "\nTabla de multiplicar del " << num << ":" << endl;
    for (int i = 1; i <= 10; i++) {
        cout << num << " x " << i << " = " << num * i << endl;
    }

    return 0;
}
